{
  "name": "af-frontend",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "prestart": "node scripts/generate-privacy-policy.js && node scripts/generate-terms.js && node scripts/generate-ai-notice.js && node scripts/generate-licenses-md.js",
    "start": "expo start -c",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web -c",
    "prebuild": "node scripts/generate-privacy-policy.js && node scripts/generate-terms.js && node scripts/generate-ai-notice.js && node scripts/generate-licenses-md.js && expo prebuild",
    "prebuild:ios": "node scripts/generate-privacy-policy.js && node scripts/generate-terms.js && node scripts/generate-ai-notice.js && node scripts/generate-licenses-md.js && expo prebuild --platform ios --clean",
    "prebuild:android": "node scripts/generate-privacy-policy.js && node scripts/generate-terms.js && node scripts/generate-ai-notice.js && node scripts/generate-licenses-md.js && expo prebuild --platform android --clean",
    "generate:privacy": "node scripts/generate-privacy-policy.js",
    "generate:terms": "node scripts/generate-terms.js",
    "generate:ai-notice": "node scripts/generate-ai-notice.js",
    "generate:licenses": "node scripts/generate-licenses-md.js",
    "generate:all": "node scripts/generate-privacy-policy.js && node scripts/generate-terms.js && node scripts/generate-ai-notice.js && node scripts/generate-licenses-md.js",
    "build:ios": "eas build --platform ios --profile production",
    "build:android": "eas build --platform android --profile production",
    "submit:ios": "eas submit --platform ios --profile production",
    "submit:android": "eas submit --platform android --profile production",
    "testflight": "eas build --platform ios --profile production --auto-submit"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@lottiefiles/dotlottie-react": "^0.13.5",
    "@orrery/core": "^0.3.0",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/native": "^7.1.8",
    "@tanstack/react-query": "^5.90.21",
    "expo": "~54.0.33",
    "expo-application": "^7.0.8",
    "expo-clipboard": "^8.0.8",
    "expo-constants": "~18.0.13",
    "expo-dev-client": "~6.0.20",
    "expo-device": "^8.0.10",
    "expo-font": "~14.0.11",
    "expo-linear-gradient": "^15.0.8",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.23",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-tracking-transparency": "^6.0.8",
    "expo-web-browser": "~15.0.10",
    "lottie-react-native": "^7.3.6",
    "lunar-javascript": "^1.7.7",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-css-interop": "^0.2.1",
    "react-native-google-mobile-ads": "^16.0.3",
    "react-native-markdown-display": "^7.0.2",
    "react-native-reanimated": "~4.1.6",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "react-test-renderer": "19.1.0",
    "tailwindcss": "^3.4.19",
    "typescript": "~5.9.2"
  },
  "private": false,
  "pnpm": {
    "patchedDependencies": {
      "@orrery/core@0.3.0": "patches/@orrery__core@0.3.0.patch"
    }
  }
}
